    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "anakkarekkeant ninre raksitav pravartticcat ennane enn ni kantille"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "avarute tantram avan pilavilakkiyille"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "kuttankuttamayikkeantulla paksikale avarute nerkk avan ayakkukayum ceytu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "cuttupaluppicca kalimankallukalkeant avare eriyunnataya"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "annane avan avare tinneatukkappetta vaikkeal turumpupealeyakki"
        }
    ]
