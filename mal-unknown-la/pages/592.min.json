{"pages":[{"chapter":87,"verse":16,"text":"pakse, ninnal aihikajivitattinn kututal pradhan'yam nalkunnu"},{"chapter":87,"verse":17,"text":"paraleakamakunnu erravum uttamavum nilanilkkunnatum"},{"chapter":87,"verse":18,"text":"tirccayayum it adyatte etukalil tanneyunt‌"},{"chapter":87,"verse":19,"text":"atayat ibrahiminreyum musayuteyum etukalil"},{"chapter":88,"verse":1,"text":"(nabiye,) a mutunna sambhavatte sambandhicca varttamanam ninakk vannukittiyea"},{"chapter":88,"verse":2,"text":"anne divasam cila mukhannal talmakanikkunnatum"},{"chapter":88,"verse":3,"text":"paniyetutt ksiniccatumayirikkum"},{"chapter":88,"verse":4,"text":"cuteriya agniyil ava pravesikkunnatan‌"},{"chapter":88,"verse":5,"text":"cuttutilakkunna oru uravil ninn avarkku kutippikkappetunnatan‌"},{"chapter":88,"verse":6,"text":"lari'il ninnallate avarkk yatearu aharavumilla"},{"chapter":88,"verse":7,"text":"at peasanam nalkukayilla. visappin samanamuntakkukayumilla"},{"chapter":88,"verse":8,"text":"cila mukhannal annu tututtu minuttatayirikkum"},{"chapter":88,"verse":9,"text":"avayute prayatnattepparri trptiyatannavayumayirikkum"},{"chapter":88,"verse":10,"text":"unnatamaya svargattil"},{"chapter":88,"verse":11,"text":"avite yatearu nirart'thakamaya vakkum avar kelkkukayilla"},{"chapter":88,"verse":12,"text":"atil oluki keantirikkunna aruviyunt‌"},{"chapter":88,"verse":13,"text":"atil uyarttivekkappetta kattilukalum"},{"chapter":88,"verse":14,"text":"tayyarakki vekkappetta keappakalum"},{"chapter":88,"verse":15,"text":"aniyayi vekkappetta talayanakalum"},{"chapter":88,"verse":16,"text":"viriccuvekkappetta paravatanikalumunt‌"},{"chapter":88,"verse":17,"text":"ottakattinre nerkk avar neakkunnille? at ennane srstikkappettirikkunnu enn‌"},{"chapter":88,"verse":18,"text":"akasattekk (avar neakkunnille?) at ennane uyarttappettirikkunnu enn‌"},{"chapter":88,"verse":19,"text":"parvvatannalilekk (avar neakkunnille?) ava ennane nattinirttappettirikkunnu venn‌"},{"chapter":88,"verse":20,"text":"bhumiyilekk (avar neakkunnille?) at ennane parattappettirikkunnuvenn‌"},{"chapter":88,"verse":21,"text":"atinal (nabiye,) ni ulbeadhippikkuka. ni oru ulbeadhakan matramakunnu"},{"chapter":88,"verse":22,"text":"ni avarute mel adhikaram celuttentavanalla"},{"chapter":88,"verse":23,"text":"pakse, vallavanum tirinnukalayukayum, avisvasikkukayum ceyyunna paksam"},{"chapter":88,"verse":24,"text":"allahu avane erravum valiya siksa siksikkunnatan‌"},{"chapter":88,"verse":25,"text":"tirccayayum nam'mute atuttekkan avarute matakkam"},{"chapter":88,"verse":26,"text":"pinnit‌, tirccayayum nam'mute badhyatayan avarute vicarana"}]}