{
    "pages": [
        {
            "chapter": 77,
            "verse": 20,
            "text": "nis'sarappetta oru dravakattil ninn ninnale nam srsticcille"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "ennitt nam atine bhadramaya oru sanketattil veccu"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "niscitamaya oru avadhi vare"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "annane nam (ellam) nirnayiccu. appeal nam etra nalla nirnayakkaran"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "bhumiye nam ulkeallunnatakkiyille"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "mariccavareyum jiviccirikkunnavareyum"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "atil unnatannalayi uraccunilkkunna parvvatannale nam vekkukayum ceytirikkunnu. ninnalkku nam svacchajalam kutikkan tarikayum ceytirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "(he, satyanisedhikale,) enteannineyayirunnea ninnal nisedhiccu talliyirunnat atilekk ninnal peayi kkealluka"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "munn sakhakalulla oru taram tanalilekk ninnal peayikkealluka"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "at tanal nalkunnatalla. tijvalayil ninn sanraksanam nalkunnatumalla"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "tirccayayum at (narakam) valiya kettitam peale uyaramulla tippeari terippiccu keantirikkum"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "at (tippeari) mannaniramulla ottaka kuttannaleppealeyayirikkum"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "avar mintattataya divasamakunnu it‌"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "avarkk olikalivu beadhippikkan anuvadam nalkappetukayumilla"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "(annavareat parayappetum:) tirumanattinre divasamanit‌. ninnaleyum purvvikanmareyum nam ita orumiccukuttiyirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "ini ninnalkk valla tantravum prayeagikkanuntenkil a tantram prayeagiccu kealluka"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "tirccayayum suksmata paliccavar (svargattil) tanalukalilum aruvikalkkitayilumakunnu"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "avar istappetunna tarattilulla palavargannalkkitayilum"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "(avareat parayappetum:) ninnal pravartticcirunnatinre phalamayi ahladatteate ninnal tinnukayum kutikkukayum ceytukealluka"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "tirccayayum nam aprakaramakunnu sad‌vrttarkk pratiphalam nalkunnat‌"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "(avareat parayappetum:) ninnal alpam tinnukayum sukhamanubhavikkukayum ceytu kealluka. tirccayayum ninnal kurravalikalakunnu"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "avareat kumpitu enn parayappettal avar kumpitukayilla"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "ini itin (khur'ann‌) sesam etearu varttamanattilan avar visvasikkunnat‌"
        }
    ]
}