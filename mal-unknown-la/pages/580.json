{
    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "ratriyil ni avane pranamikkukayum dirghamaya nisavelayil avane prakirttikkukayum ceyyuka"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "tirccayayum ikkuttar ksanikamaya aihikajivitatte istappetunnu. bharameriya oru divasattinre karyam avar tannalute purakil vittukalayukayum ceyyunnu"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "naman avare srstikkukayum avarute sariraghatana balappetuttukayum ceytat‌. nam uddesikkunna paksam avarkk tulyarayittullavare nam avarkku pakaram keantu varunnatuman‌"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "tirccayayum it oru ulbeadhanamakunnu. akayal vallavanum uddesikkunna paksam tanre raksitavinkalekkulla margam svikariccu keallatte"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "allahu uddesikkunna paksamallate ninnal uddesikkukayilla. tirccayayum allahu sarvvajnanum yuktimanumakunnu"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "avan uddesikkunnavare avanre karunyattil avan pravesippikkunnatan‌. akramakarikalkkavatte avan vedanayeriya siksa orukki veccirikkunnu"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "tutarettutare ayakkappetunnavayum"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "saktiyayi annatikkunnavayum"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "parakke vyapippikkunnavayum"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "vertiriccu vivecanam ceyyunnavayum"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "divyasandesam ittukeatukkunnavayumayittullavaye tanneyakunnu satyam"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "oru olikalivayikeantea takkitayikkeantea"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "tirccayayum ninnaleat takkit ceyyappetunna karyam sambhavikkunnatu tanneyakunnu"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "naksatrannalute prakasam maykkappetukayum"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "akasam pilarttappetukayum"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "parvvatannal peatikkappetukayum"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "dutanmarkk samayam nirnayiccu keatukkappetukayum ceytal"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "etearu divasattekkan avarkk avadhi niscayikkappettirikkunnat‌"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "tirumanattinre divasattekk‌"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "a tirumanattinre divasam entanenn ninakkariyumea"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "anne divasam nisedhiccu talliyavarkkakunnu nasam"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "purvvikanmare nam nasippiccu kalannille"
        },
        {
            "chapter": 77,
