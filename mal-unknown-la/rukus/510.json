{
    "rukus": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "he, putaccu mutiyavane"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "elunnerr (janannale) takkit ceyyuka"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "ninre raksitavine mahatvappetuttukayum"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "ninre vastrannal sud'dhiyakkukayum"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "papam vetiyukayum ceyyuka"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "kututal nettam keaticcu keant ni audaryam ceyyarut‌"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "ninre raksitavinu venti ni ksama kaikkealluka"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "ennal kahalattil mulakkappettal"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "ann at oru prayasakaramaya divasamayirikkum"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "satyanisedhikalkk eluppamullatallatta oru divasam"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "enneyum nan ekanayikkeant srsticca oruttaneyum vittekkuka"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "avann nan samrd'dhamaya sampatt untakki keatukkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "sannad'dharayi nilkkunna santatikaleyum"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "avannu nan nalla sekaryannal ceytu keatukkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "pinneyum nan kututal keatukkanamenn avan meahikkunnu"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "alla, tirccayayum avan nam'mute drstantannaleat matsaryam kanikkunnavanayirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "prayasamulla oru kayarram kayaran nam valiye avane nirbandhikkunnatan‌"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "tirccayayum avaneannu cinticcu, avaneannu kanakkakkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "atinal avan nasikkatte. ennaneyanavan kanakkakkiyat‌"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "vintum avan nasikkatte, ennaneyanavan kanakkakkiyat‌"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "pinnit avaneannu neakki"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "pinne avan mukham culikkukayum mukham karuppikkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "pinne avan pinneatt marukayum ahankaram natikkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "ennittavan parannu: it (aril ninnea) ud'dharikkappetunna maranamallate marreannumalla"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "it manusyanre vakkallate marreannumalla"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "valiye nan avane sakharil (narakattil) itt erikkunnatan‌"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "sakhar ennal entanenn ninakkariyumea"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "at onnum bakkiyakkukayea vittukalayukayea illa"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "at teali karicc rupam marrikkalayunnatan‌"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "atinre melneattattin patteampat perunt‌"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "narakattinre melneattakkarayi nam malakkukale matraman niscayiccirikkunnat‌. avarute ennatte nam satyanisedhikalkk oru pariksanam matramakkiyirikkunnu. vedam nalkappettittullavarkk drdhabeadhyam varuvanum satyavisvasikalkk visvasam vard'dhikkanum vedam nalkappettavarum satyavisvasikalum sansayattilakappetatirikkanum allahu entearu upamayan itu keant uddesiccittullatenn hrdayannalil reagamullavarum satyanisedhikalum parayuvanum ventiyatre at‌. aprakaram allahu tan uddesikkunnavare pilappikkukayum, tan uddesikkunnavare nervaliyilakkukayum ceyyunnu. ninre raksitavinre sain'yannale avanallate marrarum ariyukayilla. it manusyarkk oru ulbeadhanamallate marreannumalla"
        }
    ]
}