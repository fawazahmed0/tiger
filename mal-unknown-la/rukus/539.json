{
    "rukus": [
        {
            "chapter": 97,
            "verse": 1,
            "text": "tirccayayum nam itine (khur'anine) nirnayattinre ratriyil avatarippiccirikkunnu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "nirnayattinre ratri ennal entanenn ninakkariyamea"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "nirnayattinre ratri ayiram masattekkal uttamamakunnu"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "malakkukalum atmavum avarute raksitavinre ellakaryatte sambandhiccumulla uttaravumayi a ratriyil iranni varunnu"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "prabhateadayam vare at samadhanamatre"
        }
    ]
}