        {
            "chapter": 89,
            "verse": 1,
            "text": "prabhatam tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "pattu ratrikal tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "irattayum orrayum tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "ratri sancaricc keantirikke at tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "atil (mel parannavayil) karyabeadhamullavann satyattin vakayuneta"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "ad samudayatte keant ninre raksitav entu ceytuvenn ni kantille"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "atayat tunukalute utamakalaya iram geatratte keant‌"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "tattulyamayitteann rajyannalil srstikkappettittillatta geatram"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "tal‌varayil paravetti kettitamuntakkiyavaraya thamud geatrattekkeantum"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "anikalute alaya phir'aunekkeantum"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "natukalil atikramam pravarttikkukayum"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "avite kulappam vard'dhippikkukayum ceytavaranavar"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "atinal ninre raksitav avarute mel siksayute cam'matti varsiccu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "tirccayayum ninre raksitav patiyirikkunna sthanattu tanneyunt‌"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "ennal manusyane avanre raksitav pariksikkukayum annane avane adarikkukayum avan sekhyam nalkukayum ceytal avan parayum; enre raksitav enne adariccirikkunnu enn‌"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "ennal avane (manusyane) avan pariksikkukayum ennittavanre upajivanam itunniyatakkukayum ceytal avan parayum; enre raksitav enne apamaniccirikkunnu enn‌"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "alla, pakse ninnal anathaye adarikkunnilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "pavappettavanre aharattin ninnal preatsahanam nalkunnumilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "anantaravakasa svatt ninnal varikkutti tinnukayum ceyyunnu"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "dhanatte ninnal amitamaya teatil snehikkukayum ceyyunnu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "alla, bhumi peatipeatiyayi peatikkappetukayum"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "ninre raksitavum, aniyaniyayi malakkukalum varukayum"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "ann narakam keantu varappetukayum ceytal! anne divasam manusyann orma varunnatan‌. eviteninnanavann orma varunnat‌"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "avan parayum. ayyea, nan enre jivitattinu venti munkutti (salkarm'mannal) ceytuveccirunnenkil etra nannayirunnene"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "appeal anne divasam allahu siksikkunnaprakaram oralum siksikkukayilla"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "avan piticcu bandhikkunnat peale oralum piticcu bandhikkunnatumalla"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "he; samadhanamatanna atmave"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ni ninre raksitavinkalekk trptippettukeantum, trpti labhiccu keantum matannikkealluka"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "ennitt enre atiyanmarute kuttattil pravesiccu kealluka"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "enre svargattil pravesiccu kealluka"
        }
    ]
