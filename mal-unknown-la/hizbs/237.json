            "verse": 2,
            "text": "anne divasam cila mukhannal talmakanikkunnatum"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "paniyetutt ksiniccatumayirikkum"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "cuteriya agniyil ava pravesikkunnatan‌"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "cuttutilakkunna oru uravil ninn avarkku kutippikkappetunnatan‌"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "lari'il ninnallate avarkk yatearu aharavumilla"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "at peasanam nalkukayilla. visappin samanamuntakkukayumilla"
        },
