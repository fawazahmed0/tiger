{"rukus":[{"chapter":104,"verse":1,"text":"kuttuvākk paṟayunnavanuṁ avahēḷikkunnavanumāya ēteārāḷkkuṁ nāśaṁ"},{"chapter":104,"verse":2,"text":"atāyat dhanaṁ śēkharikkukayuṁ at eṇṇinēākkikkeāṇṭirikkukayuṁ ceyyunnavan‌"},{"chapter":104,"verse":3,"text":"avanṟe dhanaṁ avan śāśvata jīvitaṁ nalkiyirikkunnu enn avan vicārikkunnu"},{"chapter":104,"verse":4,"text":"nis'sanśayaṁ, avan hutvamayil eṟiyappeṭuka tanne ceyyuṁ"},{"chapter":104,"verse":5,"text":"hutvama ennāl entāṇenn ninakkaṟiyāmēā"},{"chapter":104,"verse":6,"text":"at allāhuvinṟe jvalippikkappeṭṭa agniyākunnu"},{"chapter":104,"verse":7,"text":"hr̥dayaṅṅaḷilēkk kattippaṭarunnatāya"},{"chapter":104,"verse":8,"text":"tīrccayāyuṁ at avaruṭe mēl aṭaccumūṭappeṭunnatāyirikkuṁ"},{"chapter":104,"verse":9,"text":"nīṭṭiyuṇṭākkappeṭṭa stambhaṅṅaḷilāyikkeāṇṭ‌"}]}